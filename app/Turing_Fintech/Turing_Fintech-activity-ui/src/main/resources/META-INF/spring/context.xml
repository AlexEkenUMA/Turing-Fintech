<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                      http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="ExampleServiceIcon"
		class="Turing.Turing_Fintech.ui.serviceprovider.ExampleServiceIcon" />

	<bean id="ExampleServiceProvider"
		class="Turing.Turing_Fintech.ui.serviceprovider.ExampleServiceProvider" />

	<bean id="ExampleConfigureMenuAction"
		class="Turing.Turing_Fintech.ui.menu.ExampleConfigureMenuAction">
		<property name="editManager" ref="editManager" />
		<property name="fileManager" ref="fileManager" />
		<property name="activityIconManager" ref="activityIconManager" />
		<property name="serviceDescriptionRegistry" ref="serviceDescriptionRegistry" />
		<property name="serviceRegistry" ref="serviceRegistry" />
	</bean>

	<bean id="ExampleActivityContextViewFactory"
		class="Turing.Turing_Fintech.ui.view.ExampleActivityContextViewFactory">
		<property name="editManager" ref="editManager" />
		<property name="fileManager" ref="fileManager" />
		<property name="activityIconManager" ref="activityIconManager" />
		<property name="serviceDescriptionRegistry" ref="serviceDescriptionRegistry" />
		<property name="serviceRegistry" ref="serviceRegistry" />
	</bean>

</beans>
